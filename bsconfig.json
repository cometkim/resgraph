{
  "name": "resgraph",
  "version": "0.1.1",
  "uncurried": true,
  "sources": [
    {
      "dir": "cli",
      "subdirs": true
    },
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".mjs",
  "bs-dependencies": [
    "@rescript/core",
    "rescript-nodejs",
    "@glennsl/rescript-fetch"
  ],
  "bsc-flags": ["-open NodeJs", "-open RescriptCore"]
}
